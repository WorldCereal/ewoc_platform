apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: app
  namespace: vdm
  annotations:
    kubernetes.io/ingress.class: kong
    konghq.com/strip-path: "true"
spec:
  rules:
  - host: vdm.VALUE1
    http:
      paths:
      - path: /backend
        backend:
          serviceName: be
          servicePort: 9850
      - path: /
        backend:
          serviceName: app-wcpdr
          servicePort: 9000