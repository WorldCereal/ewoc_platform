
apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: graylog
  namespace: logging
  annotations:
    konghq.com/strip-path: "false"
    kubernetes.io/ingress.class: kong
spec:
  rules:
  - host: graylog.VALUE1
    http:
      paths:
      - backend:
          serviceName: graylog
          servicePort: 9000
        path: /
        pathType: Prefix

